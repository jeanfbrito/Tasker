<div class="well" style="padding: 8px 0; background: white; padding: 10px;">
	<h1>
		Projects
	</h1>

	<%= render "newprojectform" %>

	<ul class="tasks zebra-list">
		<% @projects.each do |project| %>
		<li>
			<input type="checkbox"> 
			<span class="title"><%= link_to project.title, project %> 
			</span> 
			<span class="meta">Created <em>1 week ago</em> by <em>Jill</em></span>
		</li>
		<% end %>
	</ul>
</div>	